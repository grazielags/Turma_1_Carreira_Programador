algoritmo "XYZD"
// Função :
// Autor :
// Data : 19/08/2016
// Seção de Declarações 
var
   x,y,z,d : inteiro
inicio
      x <-20
      y <-5
      z <- x+y
      d <- x-y
      escreval("O valor de x e", x)
      escreval("O valor de y e", y)
      escreval("O valor de z é soma de x + y",z)
      escreval("O valor de d é a subtração de x - y",d)

// Seção de Comandos 
fimalgoritmo
