algoritmo "Media de 2 notas"
// Função :
// Autor :
// Data : 8/27/2016
// Seção de Declarações 
var
   nota1, nota2, media : real
inicio
      escreva("Digite a nota 1: ")
              leia(nota1)
      escreva("Digite a nota 2: ")
              leia(nota2)
                   media:= (nota1 + nota2)/2
      escreva("A media é: ", media)

fimalgoritmo
