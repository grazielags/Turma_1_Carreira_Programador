algoritmo "posicoes"

var

   valor: vetor [0..4] de inteiro
   i: inteiro

inicio

      para i de 0 ate 4 passo 1 faca

           escreval ("[",i, "] - Entre com o valor: ")
           leia (valor[i])
      fimpara

      para i de 0 ate 4 passo 1 faca
           escreval ("Na posição[",i, "] o valor é: ", valor[i])
      fimpara
fimalgoritmo
