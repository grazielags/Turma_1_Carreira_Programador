algoritmo "numero_maior"

var

   num1, num2: inteiro

inicio
      escreva ("Digite o primeiro número: ")
      leia (num1)
      escreva ("Digite a segundo número: ")
      leia (num2)

      se num1 > num2 entao
         escreva ("O número maior é o ", num1)
      senao
           escreva ("O número maior é o ", num2)
      fimse

fimalgoritmo
