algoritmo "temperatura"

var
   c, f, c_temp: real

inicio
      escreva ("Digite a temperatura em Graus Celsius: ")
      leia (c)

      c_temp <- ((9 * c) + 160))
      f <- (c_temp / 5)
      
      escreva ("A temperatura: ", c)
      escreva (" Graus Celsius")
      escreva (" em Fahrenheit é: ", f)

fimalgoritmo
