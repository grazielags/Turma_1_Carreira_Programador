algoritmo "Exercicio 1 - Modulo 2"

var
i, ii, iii, iv, v, md: inteiro

inicio
escreval ("Diga 5 idades:")
escreval ("Entre com i1: ")
leia (i)
escreval ("Entre com i2: ")
leia (ii)
escreval ("Entre com i3: ")
leia (iii)
escreval ("Entre com i4: ")
leia (iv)
escreval ("Entre com i5: ")
leia (v)

md <- (i + ii + iii + iv + v) div 5
escreval ("A média das 5 idades é: ", md)

fimalgoritmo
