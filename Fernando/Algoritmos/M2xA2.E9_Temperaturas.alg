algoritmo "M2xA2.E9_Temperaturas"
// Função :
// Autor :
// Data : 27/08/2016
// Seção de Declarações 
var
  f, c: real
  
inicio
      Escreval ("Digite a temperatura em graus Celsius: ")
      leia (c)
      Escreval
      f <- ((9*c) + 160)) div 5
      Escreval ("A temperatura em graus Fahrenheit é: ", f)
fimalgoritmo
